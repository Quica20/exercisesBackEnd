<div>
    <h1>PRODUCTOS</h1>
    {{#each docs}}
        <div>
            <p>----------------------------------------</p>
            <h3>{{this.title}}</h3>
            <p>Description:{{this.description}}</p>
            <p>Price:{{this.price}}</p>
        </div>
    {{/each}}
    <tr>
        <td>
            {{#if hasPrevPage}}
                    <a href={{this.prevLink}}>  Anterior </a>
                    {{/if}}
                    &nbsp;
                    {{this.page}}
                    &nbsp;
                    {{#if hasNextPage}}
                        <a href={{this.nextLink}}>  Siguiente  </a>
                    {{/if}} 
                </td>
    </tr>
</div>